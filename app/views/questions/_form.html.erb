<h1><%= question_header(test: @test, question: @question) %></h1>
<%= render 'shared/errors', resource: @question %>
<%= form_with(model: [@test, @question], local: true) do |f|%>
  <p>
    <%= f.label(:body, 'Тело вопроса') %>
    <%= f.text_field(:body) %>
  </p>
  <%= f.submit(value_submit) %>
<% end %>
