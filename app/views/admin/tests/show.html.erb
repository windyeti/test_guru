<h1>Test <%= @test.title %></h1>
<p>Это тест уровня <%= @test.level %></p>
<% if @test.questions.present? %>
  <table>
    <tr>
      <th>ID</th>
      <th>Сам вопрос</th>
      <th>Из теста</th>
      <th>Сложность</th>
      <th colspan="3">Actions</th>
    </tr>
    <%= render partial: 'questions/question', collection: @test.questions %>
    <%#= render @test.questions %>
  </table>
  <p>
    | <%= link_to 'Back', admin_tests_path %> |
    | <%= link_to 'Создать вопрос', new_test_question_path(@test) %> |
  </p>
<% else %>
  <p>This test has no questions.</p>
<% end %>
