<h1><%= t('.header', title: @test.title) %></h1>
<p><%= t('.header_level', level: @test.level) %></p>
<% if @test.questions.present? %>
  <table>
    <tr>
      <th><%= t('.id') %></th>
      <th><%= t('.body_question') %></th>
      <th><%= t('.from_test') %></th>
      <th><%= t('.level') %></th>
      <th colspan="3"><%= t('.actions') %></th>
    </tr>
    <%#= render partial: 'questions/question', collection: @test.questions %>
    <%= render @test.questions %>
  </table>
<% else %>
  <p><%= t('.no_question') %></p>
<% end %>
<p>
  | <%= link_to t('.back'), admin_tests_path %> |
  | <%= link_to t('.create_new_question'), new_admin_test_question_path(@test) %> |
</p>
