<%= form_with(model: [:admin, @question, @answer], local: true) do |form| %>
  <p>
    <%= form.label(:body) %>
    <%= form.text_area(:body, size: '50x10') %>
  </p>
  <p>
    <%= form.label(:correct) %>
    <%= form.check_box(:correct) %>
  </p>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
